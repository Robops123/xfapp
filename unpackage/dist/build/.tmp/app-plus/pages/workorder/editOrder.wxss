.delete {
  position: absolute;
  right: -10px;
  top: -10px;
  width: 20px;
  height: 20px;
  border-radius: 50%;
  background: red;
  color: #333333;
  text-align: center;
  line-height: 20px;
  font-size: 16px;
}


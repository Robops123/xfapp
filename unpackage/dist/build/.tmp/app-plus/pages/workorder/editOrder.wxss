.yt-list {
  border-top: 1px solid #e8e7e7;
  border-bottom: 1px solid #e8e7e7;
  margin-top: 16rpx;
  background: #f4f0f0;
}
.yt-list-cell {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 10rpx 30rpx 10rpx 40rpx;
  line-height: 70rpx;
  position: relative;
}
.yt-list-cell.cell-hover {
    background: #fafafa;
}
.yt-list-cell.b-b:after {
    left: 30rpx;
}
.yt-list-cell .cell-icon {
    height: 32rpx;
    width: 32rpx;
    font-size: 22rpx;
    color: #fff;
    text-align: center;
    line-height: 32rpx;
    background: #f85e52;
    border-radius: 4rpx;
    margin-right: 12rpx;
}
.yt-list-cell .cell-icon.hb {
      background: #ffaa0e;
}
.yt-list-cell .cell-icon.lpk {
      background: #3ab54a;
}
.yt-list-cell .cell-tit {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
            flex: 1;
    font-size: 26rpx;
    color: dimgray;
    margin-right: 10rpx;
}
.yt-list-cell .cell-tip {
    font-size: 26rpx;
    color: #f85e52;
}
.yt-list-cell .cell-tip.disabled {
      color: #f85e52;
}
.yt-list-cell .cell-tip.active {
      color: #f85e52;
}
.yt-list-cell .cell-tip.red {
      color: #f85e52;
}
.yt-list-cell.desc-cell .cell-tit {
    max-width: 140rpx;
    -webkit-align-self: flex-start;
            align-self: flex-start;
}
.yt-list-cell.desc-cell .cell-cot {
    max-width: calc(100% - 140rpx);
    word-break: break-word;
}
.yt-list-cell .desc {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
            flex: 1;
    font-size: 28rpx;
    color: darkgray;
}
.yt-list-cell switch {
    -webkit-transform: translateX(16rpx) scale(0.84);
            transform: translateX(16rpx) scale(0.84);
}
.footer {
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 995;
  text-align: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 100%;
  height: 90rpx;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  font-size: 30rpx;
  z-index: 998;
}
.footer .price-content {
    padding-left: 30rpx;
}
.footer .price-tip {
    color: #f85e52;
    margin-left: 8rpx;
}
.footer .price {
    font-size: 36rpx;
    color: #f85e52;
}
.footer .submit {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
    width: 280rpx;
    height: 80%;
    border-radius: 10px;
    color: #fff;
    margin: 0 auto;
    font-size: 32rpx;
    background-color: #3866ef;
}
.footer .cell-pd {
    padding: 22rpx 30rpx;
}
.footer .list-pd {
    margin-top: 50rpx;
}
